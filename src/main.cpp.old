#include <Arduino.h>

int val = 0;

void setup(){
  Serial.begin(9600);
  pinMode(11,OUTPUT);
  pinMode(10,INPUT);
  Serial.println("Starting led...");
}

void loop(){
  val = digitalRead(10);
  Serial.println(val);
  digitalWrite(11,val);
